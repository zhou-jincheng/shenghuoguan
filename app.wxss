@import './styles/iconfont.wxss';

page,view,text,swiper,swiper-item,image,navigator {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}
/* 主题颜色 通过变量实现
less 中存在 变量 这个知识
原生的css和wxss也是支持 变量 */
page {
    /* 定义主题颜色 */
    --themeColor: #eb4450;
    /* 统一字体大小 假设设计稿 大小是 375px
    1px = 2 rpx
    14px = 28rpx */
    font-size: 28rpx;
}

image {
    width: 100%;
}